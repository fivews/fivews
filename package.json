{
  "name": "Storycopter",
  "version": "0.0.1",
  "main": "public/electron.js",
  "license": "BSD-3-Clause",
  "homepage": "./",
  "author": {
    "name": "Storycopter HQ",
    "email": "storycopter@gmail.com",
    "url": "https://storycopter.com"
  },
  "build": {
    "appId": "com.storycopter.app",
    "productName": "Storycopter",
    "copyright": "Copyright Â©2020 ${author}",
    "mac": {
      "category": "public.app-category.utilities",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "entitlements.mac.plist",
      "entitlementsInherit": "entitlements.mac.plist"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "afterSign": "scripts/notarize.js"
  },
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.56",
    "@reduxjs/toolkit": "1.4.0",
    "@storycopter/storycopter-gatsby-starter": "0.0.23",
    "adm-zip": "0.4.16",
    "ansi-to-react": "6.0.8",
    "electron-is": "3.0.0",
    "electron-is-dev": "1.2.0",
    "fs-extra": "9.0.1",
    "gatsby": "2.24.56",
    "hazardous": "0.3.0",
    "material-ui-popup-state": "1.6.1",
    "npm": "6.14.8",
    "npm-commands": "1.1.0",
    "polished": "3.6.6",
    "react": "16.13.1",
    "react-beautiful-dnd": "13.0.0",
    "react-color": "2.18.1",
    "react-dom": "16.13.1",
    "react-is": "16.13.1",
    "react-new-window": "0.1.2",
    "react-player": "2.6.2",
    "react-redux": "7.2.1",
    "react-scripts": "3.4.3",
    "redux": "4.0.5",
    "redux-localstorage-simple": "2.3.1",
    "redux-logger": "3.0.6",
    "sanitize-filename": "1.6.3",
    "shelljs": "0.8.4",
    "strip-ansi": "6.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@rescripts/cli": "0.0.14",
    "@rescripts/rescript-env": "0.0.12",
    "babel-loader": "8.1.0",
    "babel-plugin-styled-components": "1.11.1",
    "concurrently": "5.3.0",
    "electron": "10.1.1",
    "electron-builder": "22.8.0",
    "electron-notarize": "1.0.0",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.6",
    "husky": "4.3.0",
    "lint-staged": "10.3.0",
    "prettier": "2.1.1",
    "prettier-eslint": "11.0.0",
    "prettier-stylelint": "0.4.2",
    "styled-components": "5.2.0",
    "stylelint": "13.7.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-prettier": "1.1.2",
    "typescript": "4.0.2",
    "wait-on": "5.2.0",
    "webpack": "4.44.1"
  },
  "scripts": {
    "start": "rescripts start",
    "clean": "npx rimraf **/node_modules/",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "electron-builder -mw",
    "foo": "echo bar"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "git add"
    ],
    "*.{md,html}": [
      "prettier --write",
      "git add"
    ]
  }
}
