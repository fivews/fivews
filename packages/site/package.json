{
  "name": "@storycopter/site",
  "version": "1.0.0",
  "license": "BSD-3-Clause",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": [
        "prettier --write",
        "git add"
      ],
      "*.{md,html}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      ".cache",
      "node_modules",
      "public"
    ]
  },
  "dependencies": {
    "@material-ui/core": "4.3.0",
    "gatsby": "2.13.45",
    "gatsby-cli": "2.7.21",
    "gatsby-source-filesystem": "2.1.7",
    "gatsby-transformer-remark": "2.6.10",
    "lodash": "4.17.15",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "styled-components": "4.3.2"
  },
  "devDependencies": {
    "babel-eslint": "10.0.2",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "eslint": "6.1.0",
    "gh-pages": "2.0.1",
    "husky": "3.0.2",
    "lint-staged": "9.2.1",
    "prettier-eslint": "9.0.0",
    "prettier-stylelint": "0.4.2",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "react-hot-loader": "4.12.10",
    "stylelint-prettier": "1.1.1"
  },
  "scripts": {
    "clean:all": "rm -rf ./.cache && rm -rf ./public && rm -rf ./node_modules && rm -f ./yarn.lock && rm -f ./yarn-error.log",
    "clear:gatsby": "rm -rf ./.cache && rm -rf ./public",
    "start:lan": "./node_modules/.bin/gatsby develop -o -H $HOSTNAME -p 8000",
    "deploy": "gatsby build && cp CNAME public/CNAME && gh-pages -d public",
    "start": "./node_modules/.bin/gatsby develop",
    "build": "./node_modules/.bin/gatsby build"
  }
}
